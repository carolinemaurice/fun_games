
  <%= form_with url: games_path, method: :get, class: "d-flex" do %>
  <%= text_field_tag :query,
  params[:query],
  class: "form-control",
  placeholder: "Find a game"
  %>
  <%= text_field_tag :address,
  params[:address],
  class: "form-control",
  placeholder: "Where ?"
  %>
  <%= submit_tag "Search", class: "btn btn-secondary" %>
  <% end %>
