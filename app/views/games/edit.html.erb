<%= form_with model: @game do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :players %>
  <%= f.number_field :players %>
  <%= f.label :minimum_age %>
  <%= f.number_field :minimum_age %>
  <%= f.label :price_per_day %>
  <%= f.number_field :price_per_day %>

  <%= f.submit %>
<% end %>
